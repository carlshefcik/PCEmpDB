{
  "author": "Carl Shefcik",
  "name": "pcdb",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron.js",
  "homepage": "./",
  "build": {
    "appId": "PeerConnectionsDB",
    "extends": null,
    "files": [
      "build/**/*",
      "src/electron.js"
    ],
    "directories":{
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "electron": "^3.0.5",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.1.2",
    "react-scripts": "2.0.5"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "mysql": "^2.16.0",
    "popper.js": "^1.14.7",
    "react": "^16.6.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.5.0",
    "sqlite3": "^4.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron": "electron .",
    "package-win": "npm run build && electron-builder build --win --ia32"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
